{
  "manifest_version": 3,
  "name": "Hide AI Generated Posts on X",
  "version": "1.0",
  "description": "Hides X.com posts containing em dashes",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*", "*://*.x.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}